msgstr "se un campo è vuoto"

#: ../../source/exploring_catalog_and_datasets/03_searching_the_data/search.rst:41
msgid "#null(id_field)"
msgstr "#null(id_field)"

#: ../../source/exploring_catalog_and_datasets/03_searching_the_data/search.rst:42
msgid "``#null(referer)``"
msgstr "``#null(referer)``"

