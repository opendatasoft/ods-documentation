#: ../../source/publishing_data/05_processing_data/processors/nominatim_geocoder.rst:25
msgid "Postal code"
msgstr "Código postal"

#: ../../source/publishing_data/05_processing_data/processors/nominatim_geocoder.rst:26
msgid "Field containing the postal code"
msgstr "Campo que contiene el código postal."

#: ../../source/publishing_data/05_processing_data/processors/nominatim_geocoder.rst:28
msgid "City"
msgstr "Ciudad"

#: ../../source/publishing_data/05_processing_data/processors/nominatim_geocoder.rst:29
