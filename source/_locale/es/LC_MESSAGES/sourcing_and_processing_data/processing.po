msgstr "00, 01... 23"

#: ../../source/sourcing_and_processing_data/processing.rst:989
msgid "%I"
msgstr "%I"

#: ../../source/sourcing_and_processing_data/processing.rst:990
msgid "Hour (12-hour clock) as a zero-padded decimal number."
msgstr "Hora (reloj de 12 horas) como número decimal rellenado con 0."

