#: ../../source/sourcing_and_processing_data/processing.rst:974
msgid "%B"
msgstr "%B"

#: ../../source/sourcing_and_processing_data/processing.rst:975
msgid "Month as locale’s full name."
msgstr "Mes como nombre completo en la configuración regional."

#: ../../source/sourcing_and_processing_data/processing.rst:976
msgid "January, February, ..., December"
msgstr "January, February... December"

#: ../../source/sourcing_and_processing_data/processing.rst:977
msgid "%m"
msgstr "%m"

#: ../../source/sourcing_and_processing_data/processing.rst:978
msgid "Month as a zero-padded decimal number."
msgstr "Mes como número decimal rellenado con 0."

#: ../../source/sourcing_and_processing_data/processing.rst:979
#: ../../source/sourcing_and_processing_data/processing.rst:991
msgid "01, 02, ..., 12"
msgstr "01, 02... 12"

#: ../../source/sourcing_and_processing_data/processing.rst:980
msgid "%y"
msgstr "%y"

#: ../../source/sourcing_and_processing_data/processing.rst:981
