#: ../../source/sourcing_and_processing_data/processing.rst:926
msgid "Integer"
msgstr "Ganzzahl"

#: ../../source/sourcing_and_processing_data/processing.rst:923
msgid "0, 8, 16 ..."
msgstr "0, 8, 16 ..."

#: ../../source/sourcing_and_processing_data/processing.rst:924
msgid "Stop bit offset"
msgstr "Bitversatz stoppen"

#: ../../source/sourcing_and_processing_data/processing.rst:925
msgid "The ending offset corresponding of the position of the last bit"
msgstr "Der Abschlussversatz entsprechend der Position des letzten Bits"

