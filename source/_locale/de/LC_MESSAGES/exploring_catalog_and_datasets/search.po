#: ../../source/exploring_catalog_and_datasets/search.rst:2
msgid "Searching in the data"
msgstr "Suchen in Daten"

#: ../../source/exploring_catalog_and_datasets/search.rst:7
msgid "Search for records"
msgstr "Suche nach Einträgen"

#: ../../source/exploring_catalog_and_datasets/search.rst:8
msgid "Query"
msgstr "Anfrage"

#: ../../source/exploring_catalog_and_datasets/search.rst:9
msgid "Example (based on ods-api-monitoring dataset)"
msgstr "Beispiel (basierend auf Datensatz ods-api-Überwachung)"

#: ../../source/exploring_catalog_and_datasets/search.rst:10
msgid "containing a value"
msgstr "mit einem Wert"

#: ../../source/exploring_catalog_and_datasets/search.rst:11
msgid "value"
msgstr "Wert"

#: ../../source/exploring_catalog_and_datasets/search.rst:12
msgid "``explore``"
msgstr "``explore``"

#: ../../source/exploring_catalog_and_datasets/search.rst:14
msgid "returns every record containing the string explore"
msgstr "gibt alle Einträge mit der Zeichenfolge explore zurück "

#: ../../source/exploring_catalog_and_datasets/search.rst:15
msgid "containing a value in a specific field"
msgstr "mit einem Wert in einem bestimmten Feld"

#: ../../source/exploring_catalog_and_datasets/search.rst:16
msgid "id_field:value"
msgstr "id_field:value"

#: ../../source/exploring_catalog_and_datasets/search.rst:17
msgid "``action:explore``"
msgstr "``action:explore``"

#: ../../source/exploring_catalog_and_datasets/search.rst:19
msgid ""
"returns every record containing the string explore in the column named "
"action"
msgstr ""
"gibt alle Einträge mit der Zeichenfolge explore in der Spalte namens action "
"zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:20
msgid "containing several values"
msgstr "mit mehreren Werten"

#: ../../source/exploring_catalog_and_datasets/search.rst:21
msgid "value1 AND value2"
msgstr "Wert1 UND Wert2"

#: ../../source/exploring_catalog_and_datasets/search.rst:22
msgid "``explore AND search``"
msgstr "``explore AND search``"

#: ../../source/exploring_catalog_and_datasets/search.rst:24
msgid "returns every record containing both explore and search"
msgstr "gibt alle Einträge mit den Zeichenfolgen explore sowie search zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:25
msgid "containing at least one of the values"
msgstr "mit mindestens einem der Werte"

#: ../../source/exploring_catalog_and_datasets/search.rst:26
msgid "value1 OR value2"
msgstr "Wert1 ODER Wert2"

#: ../../source/exploring_catalog_and_datasets/search.rst:27
msgid "``action OR explore``"
msgstr "``action OR explore``"

#: ../../source/exploring_catalog_and_datasets/search.rst:29
msgid "returns every record containing either explore or search"
msgstr ""
"gibt alle Einträge mit entweder der Zeichenfolge explore oder search zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:30
msgid "not containing a value"
msgstr "ohne einen Wert"

#: ../../source/exploring_catalog_and_datasets/search.rst:31
msgid "NOT value"
msgstr "OHNE Wert"

#: ../../source/exploring_catalog_and_datasets/search.rst:32
msgid "``NOT anonymous``"
msgstr "``NOT anonymous``"

#: ../../source/exploring_catalog_and_datasets/search.rst:34
msgid "returns every record which does not contain the string anonymous"
msgstr "gibt alle Einträge ohne die Zeichenfolge anonymous zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:35
msgid "containing an exact value in a field"
msgstr "mit einem genauen Wert in einem Feld"

#: ../../source/exploring_catalog_and_datasets/search.rst:36
msgid "#exact(id_field,\"value\")"
msgstr "#exact(id_field,\"value\")"

#: ../../source/exploring_catalog_and_datasets/search.rst:37
msgid "``#exact(user_id,\"anonymous\")``"
msgstr "``#exact(user_id,\"anonymous\")``"

#: ../../source/exploring_catalog_and_datasets/search.rst:39
msgid ""
"returns every record containing the exact string anonymous in the column "
"named user_id"
msgstr ""
"gibt alle Einträge mit der genauen Zeichenfolge anonymous in der Spalte "
"namens user_id zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:40
msgid "if a field is empty"
msgstr "wenn ein Feld leer ist"

#: ../../source/exploring_catalog_and_datasets/search.rst:41
msgid "#null(id_field)"
msgstr "#null(id_field)"

#: ../../source/exploring_catalog_and_datasets/search.rst:42
msgid "``#null(referer)``"
msgstr "``#null(referer)``"

#: ../../source/exploring_catalog_and_datasets/search.rst:44
msgid "returns every record which has no value in the column named referer"
msgstr ""
"gibt alle Einträge ohne einen Wert in der Spalte namens referer zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:45
msgid "where a date's field is anterior to a value"
msgstr "wo ein Datumsfeld vor einem Wert liegt"

#: ../../source/exploring_catalog_and_datasets/search.rst:46
msgid "id_date_field<=YYYY/MM/DD"
msgstr "id_date_field<=YYYY/MM/DD"

#: ../../source/exploring_catalog_and_datasets/search.rst:47
msgid "``timestamp<=2016/09``"
msgstr "``timestamp<=2016/09``"

#: ../../source/exploring_catalog_and_datasets/search.rst:49
msgid ""
"returns every record with a timestamp prior and equal to September 2016"
msgstr ""
"gibt alle Einträge mit einem Zeitstempel vor oder gleich September 2016 "
"zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:50
msgid "where a date's field is the current date minus a period"
msgstr "wo ein Datumsfeld das aktuelle Datum minus einem Zeitraum ist"

#: ../../source/exploring_catalog_and_datasets/search.rst:51
msgid "id_date_field>#now(days=-value)"
msgstr "id_date_field>#now(days=-value)"

#: ../../source/exploring_catalog_and_datasets/search.rst:52
msgid "``timestamp>#now(days=-7)``"
msgstr "``timestamp>#now(days=-7)``"

#: ../../source/exploring_catalog_and_datasets/search.rst:54
msgid ""
"returns every record with a timestamp equal to current day minus 7 days"
msgstr ""
"gibt alle Einträge mit einem dem aktuellen Tag minus 7 Tage entsprechenden "
"Zeitstempel zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:55
msgid "containing a geo field located in a specific area"
msgstr "mit einem in einem bestimmten Gebiet liegenden Geofeld"

#: ../../source/exploring_catalog_and_datasets/search.rst:56
msgid "#distance(\"latitude,longitude\",distance)"
msgstr "#distance(\"latitude,longitude\",distance)"

#: ../../source/exploring_catalog_and_datasets/search.rst:57
msgid "``#distance(48.866667,2.333333,1000)``"
msgstr "``#distance(48.866667,2.333333,1000)``"

#: ../../source/exploring_catalog_and_datasets/search.rst:59
msgid "returns every record located at 1 km from the center of Paris"
msgstr ""
"gibt alle 1 km vom Pariser Stadtzentrum entfernt liegenden Einträge zurück"

#: ../../source/exploring_catalog_and_datasets/search.rst:62
msgid ""
"For more details about the requests you can make, check out the :doc:`query "
"language reference </using_api/v1/query_language_and_geo_filtering>`."
msgstr ""
"Weitere Details über Anfragen, die Sie starten können, finden Sie unter  "
":doc:`Anfragesprachenbezug "
"</using_api/v1/query_language_and_geo_filtering>`."
